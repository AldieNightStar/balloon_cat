script/source = "extends KinematicBody2D

export var speed = 5
export var max_hp = 4

var min_w
var max_w
var min_h
var max_h

var hp : int
var _setup = false
var invincible_time = 0
var catAnimation : AnimationPlayer
var dead = false
var is_player = true
var playable = false

# Joystick variables
var j_used = false
var j_move_x = 0
var j_move_y = 0

signal died()
signal change_invincible(isInvincible)

func _ready():
	max_hp = G.cat_hp
	call_deferred(\"setup\")
	
func setup():
	hp = max_hp
	var p = get_parent()
	min_w = p.get_node(\"Min_W\").position.x
	min_h = p.get_node(\"Min_H\").position.y
	max_w = p.get_node(\"Max_W\").position.x
	max_h = p.get_node(\"Max_H\").position.y
	_setup = true
	_set_balloons(hp)
	catAnimation = $g/ani
	catAnimation.play(\"Idle\")
	catAnimation.connect(\"animation_finished\", self, \"cat_ani_finish\")
	G.connect(\"_on_joystick_change\", self, \"_on_joystick_change\")

func _physics_process(delta):
	if _setup and !dead:
		if playable:
			kb_process(delta)
			if j_used:
				move(j_move_x, j_move_y)
		else:
			# Go from left screen side
			var c = move_and_collide(Vector2(2, 0))
			if c and c.collider:
				if c.collider.has_method(\"touch\"):
					c.collider.touch(self)
				add_collision_exception_with(c.collider)
	if dead:
		rotation += 0.05
		position += Vector2(0, speed)

func kb_process(delta):
	var down_key = Input.is_action_pressed(\"ui_down\")
	var xMove = 0
	var yMove = 0
	if Input.is_action_pressed(\"ui_left\"):
		xMove = -speed
	elif Input.is_action_pressed(\"ui_right\"):
		xMove = speed
	if Input.is_action_pressed(\"ui_up\"):
		yMove = -speed
	elif Input.is_action_pressed(\"ui_down\"):
		yMove = speed
	if Input.is_action_just_pressed(\"ui_accept\"):
		if !down_key:
			fire()
		else:
			fire_down()
	move(xMove, yMove)
	
func move(xMove, yMove):
	var c = move_and_collide(Vector2(xMove, yMove))
	if c:
		if c.collider.has_method(\"touch\"):
			c.collider.touch(self)
	protect_positions()

func protect_positions():
	if position.x > max_w:
		position.x = max_w
	if position.x < min_w:
		position.x = min_w
	if position.y > max_h:
		position.y = max_h
	if position.y < min_h:
		position.y = min_h
	
func damage(n):
	if invincible_time > 0 or dead:
		return
	hp -= n
	if hp < 1:
		dead()
	else:
		make_invincible()
	_set_balloons(hp)
	G.play(\"blow.ogg\")
	

func dead():
	dead = true
	catAnimation.play(\"Dead\")

func add_health(n):
	if !dead:
		hp += n
		if hp > max_hp:
			hp = max_hp
		_set_balloons(hp)

func destruct():
	emit_signal(\"died\")
	queue_free()

func set_max_hp(hp):
	self.max_hp = hp
	self.hp = hp
	_set_balloons(hp)

func ivincible_time_sub():
	if (invincible_time > 0):
		invincible_time -= 1
		if (invincible_time < 1):
			emit_signal(\"change_invincible\", false)

func make_invincible(n = 2):
	if (invincible_time < 1):
		emit_signal(\"change_invincible\", true)
		if n < 1:
			n = 1
		invincible_time = n

func is_invincible():
	return invincible_time > 0

func _set_balloons(n):
	$g/Balloons.set_count(n)

func sling_fire():
	$g/hand/slingPos/sling.fire(self, self.get_parent().get_parent())

func fire():
	if catAnimation.current_animation != \"Fire\":
		sling_fire()
		catAnimation.play(\"Fire\")

func fire_down():
	if catAnimation.current_animation != \"Fire\":
		var rock = load(\"res://Objects/Falling_Rock.tscn\").instance()
		rock.global_position = global_position + Vector2(40, 0)
		get_parent().get_parent().add_child(rock)
		catAnimation.play(\"Fire\")

func cat_ani_finish(name):
	if name == \"Fire\":
		catAnimation.play(\"Idle\")
	
func change_sling(sling: Node2D):
	var hand = $g/hand/slingPos
	if !sling.has_method(\"fire\"):
		print(\"SLING HAS NO \\\"fire()\\\" function!\")
		return
	if hand.has_node(\"sling\"):
		var old_sling = hand.get_node(\"sling\")
		hand.remove_child(old_sling)
		old_sling.queue_free()
	sling.name = \"sling\"
	hand.add_child(sling)

func _ticker_timeout():
	ivincible_time_sub()

func level_holder():
	return get_parent()

func _on_joystick_change(v):
	if !j_used:
		j_used = true
	if playable and !dead:
		j_move_x = (v.x * speed) * 1.15
		j_move_y = (v.y * speed) * 1.15
"

