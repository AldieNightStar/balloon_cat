script/source = "extends KinematicBody2D

export var x_velocity = 10
export var y_speed = 10
export var damage_value = 1

func _physics_process(delta):
	var c = move_and_collide(Vector2(x_velocity, y_speed))
	if x_velocity > 0:
		x_velocity -= 1
	if c:
		var obj = c.collider
		if obj.has_method(\"damage\") and !obj.get(\"is_player\"):
			obj.damage(damage_value)
			destruct()

func destruct():
	queue_free()
"

